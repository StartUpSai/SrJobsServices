<script>

        $(function () {


            $("#regis").click(function () {
                $("#registration").show();
                $("#loggedin").hide();
            });

            $("#logback").click(function () {
                $("#loggedin").show();
                $("#registration").hide();
            });

            $("#forget-password").click(function () {
                $("#forgot").show();
                $("#loggedin").hide();
                $("#registration").hide();
            });
            $("#forsubmit").click(function () {
                $("#useralert").show();
                $("#forgot").hide();
                $("#loggedin").show();
                $("#registration").hide();
                $("#usererror").html("Password reset link Has been send to your email address");

            });
            $("#logforback").click(function () {
                $("#loggedin").show();
                $("#registration").hide();
                $("#forgot").hide();
            });


          $("#name").keypress(function () { $("#useralert").hide();})
          $("#password").keypress(function () { $("#useralert").hide(); })
       });
</script>
