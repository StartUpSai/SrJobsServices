<script>
    $(window).scroll(function () {

        if ($(this).scrollTop() > 500) {
            $('#divlog').fadeIn();
            $('#basicSuccessWidth').hide();
        }
        else {
            $('#divlog').fadeOut();
            $('#basicSuccessWidth').show();
        }
    });

    $("#userid").keypress(function () {
        $("#UserError").hide();
    });

    $("#passwordid").keypress(function () {
        $("#UserError").hide();
    });


    $(document).ready(function () {

        $('#basicSuccessWidth2').click(function () {
            if ($('#User_name').val() == '') {
                $('#UserError').show();
                $("#LogOnError").html('Please Enter User Name');
                $('#User_name').focus();
                return false;
            }
            else if ($('#Password').val() == '') {
                $('#UserError').show();
                $("#LogOnError").html('Please Enter Password');
                $('#Password').focus();
                return false;
            }
            else {
                $('#UserError').hide();
            }
        });





        var errorVal = $('#LogOnError').text();
        if (errorVal != '') {
            $('#UserError').show();
        }
        else {
            $('#UserError').hide();
        }
    });



</script>

